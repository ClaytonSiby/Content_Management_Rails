<%= error_messages_for(form.object) %>

<table summary="Subject form fields">
  <tr>
    <th>Subject ID</th>
    <td><%= form.text_field(:subject_id) %></td>
  </tr>

  <tr>
    <th>Name</th>
    <td><%= form.text_field(:name) %></td>
  </tr>

  <tr>
    <th>Position</th>
    <td><%= form.text_field(:position) %></td>
  </tr>

  <tr>
    <th>Permalink</th>
    <td><%= form.text_field(:permalink) %></td>
  </tr>

  <tr>
    <th>Visible</th>
    <td><%= form.text_field(:visible) %></td>
  </tr>

  <tr>
    <th>Content</th>
    <td><%= form.text_area(:context, size: '50x10') %></td>
  </tr>
</table>
